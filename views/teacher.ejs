<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <table border="2px">
      <input type="text" name="first_name" placeholder="First Name"></input>
      <input type="text" name="last_name" placeholder="Last Name"></input>
      <input type="text" name="email" placeholder="Email"></input>
      <input type="text" name="gender" placeholder="Gender"></input>
      <input type="submit" name="submit" ></input>
      <thead>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Gender</th>
        <th>action</th>
      </thead>
      <tbody>
        <%dataTeacher.forEach(data=>{%>
        <tr>
            <td><%=data.id%></td>
            <td><%=data.first_name%></td>
            <td><%=data.last_name%></td>
            <td><%=data.email%></td>
            <td><%=data.gender%></td>
            <td>
              <a href="/teacher/edit/:id">Edit Teacher</a>
              <a href="/teacher/edit/:id">Delete Teacher</a>
            </td>


        </tr>
        <%})%>
      </tbody>
    </table>

  </body>
</html>
