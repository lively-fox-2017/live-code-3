<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Subjects</title>
</head>
<body>
  <table>
    <tr>
      <th>Subjects ID</th>
      <th>Subjects Code</th>
      <th>Subjects Name</th>
    </tr>
    <% for(let i = 0; i < dataJsonSubjects.length; i++){ %>
    <tr>
      <td><%= dataJsonSubjects[i].id %></td>
      <td><%= dataJsonSubjects[i].subject_code %></td>
      <td><%= dataJsonSubjects[i].subject_name %></td>
      <td><a href="/subjects/edit/<%= dataJsonSubjects[i].id %>">edit</td>
      <td><a href="/subjects/delete/<%= dataJsonSubjects[i].id %>">delete</td>
    </tr>
    <% } %>
  </table>
  <form action="/subjects" method="post">
    Subject Code:<br>
    <input type="text" name="subject_code">
    <br>
    Subject Name:<br>
    <input type="text" name="subject_name">
    <br><br>
    <input type="submit" value="Submit">
  </form>
</body>
</html>
