<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>List Teacher</title>
  </head>
  <body>
    <h1>Ini halaman list teacher</h1>
    <table>
      <tr>
        <th>id</th>
        <th>first_name</th>
        <th>last_name</th>
        <th>email</th>
        <th>gender</th>
        <th>id_subject</th>
      </tr>
      <%for(var i = 0; i < dataTeacher.length; i++) {%>
      <tr>
        <td><%= dataTeacher[i].id%></td>
        <td><%= dataTeacher[i].first_name%></td>
        <td><%= dataTeacher[i].last_name%></td>
        <td><%= dataTeacher[i].email%></td>
        <td><%= dataTeacher[i].gender%></td>
        <td><%= dataTeacher[i].id_subject%></td>
        <td><a href="teacher/edit/<%=dataTeacher[i].id%>">edit</a></td>
        <td><a href="teacher/delete/<%=dataTeacher[i].id%>">delete</a></td>
      </tr>
      <%}%>
    </table>
  </body>
</html>
