<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Pages Subjects</title>
  </head>
  <body>
  <a href="/subjects">List Subjects</a>
  <a href="/teachers">Teachers</a>
  <table border='1px'>
    <h1>Daftar List Subject</h1>
    <a href="/subjects/add">Add
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Subject Code</th>
        <th colspan="3">Action</th>
      </tr>
    </thead>
    <tbody>
      <%dataSubjects.forEach(row=>{ %>
        <tr>
          <td><%= row.id%></td>
          <td><%= row.subject_name%></td>
          <td><%= row.subject_code%></td>
          <td><a href="/subjects/edit/<%= row.id%>">Edit</td>
          <td><a href="/subjects/delete/<%= row.id%>">Delete</td>
        </tr>
      <%})%>
    </tbody>
  </table>
  </body>
</html>
