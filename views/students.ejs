<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Show All Students</title>
  </head>
  <body>
    <table>
      <tr>
        <th>ID</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Gender</th>
        <th>Subjects</th>
      </tr>
      <% for (let i = 0; i < students.length; i++) {%>
        <tr>
          <td><%= students[i].id %></td>
          <td><%= students[i].first_name %></td>
          <td><%= students[i].last_name %></td>
          <td><%= students[i].email %></td>
          <td><%= students[i].gender %></td>
          <td><%= students[i].subjectsStr %></td>
        </tr>
      <%}%>
    </table>
  </body>
</html>
