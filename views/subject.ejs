<html>
    <head>
    </head>
    <body>
        <table>
            <%data.forEach(data => {%>
                <tr>
                    <td><%=data.id%></td>
                    <td><%=data.subject_name%></td>
                    <td><%=data.subject_code%></td>
                    <%data.students.forEach(students => {%>
                        <td><%=students%></td>
                    <%})%>
                    <td><a href='/subject/update/<%=data.id%>'>edit</a></td>
                    <td><a href='/subject/delete/<%=data.id%>'>delete</a></td>
                    <td><a href='/subject/assign/<%=data.id%>'>assign</a></td>
                </tr>    
                <%})%>
        </table>
        <a href ='/subject/add'>Add Subject</a>
        <a href='/'>Home</a>
    </body>
</html>