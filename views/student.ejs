<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Student</title>
  </head>
  <body>
    <a href="/">BACK</a>
    <table align="center" border="1">
      <thead>
        <tr>
          <td>ID</td>
          <td>First Name</td>
          <td>Last Name</td>
          <td>Email/td>
          <td>Gender</td>
          <td>Action</td>
        </tr>
      </thead>
      <tbody>
        <tr>
          <% dataStudent.forEach((row) => {%>
            <td><%= row.id%></td>
            <td><%= row.first_name%></td>
            <td><%= row.last_name%></td>
            <td><%= row.email%></td>
            <td><%= row.gender%></td>
            <td><a href="/student/edit/<%= row.id%>">Edit</a> | <a href="/student/delete/<%= row.id%>">Delete</a></td>
          <%})%>
        </tr>
      </tbody>
    </table><hr />

    <form action="/subject/insert" method="post">
      <label>Subject Name</label><br>
      <input type="text" name="subject_name"><br>
      <label>Subject Code</label><br>
      <input type="text" name="subject_code"><br>
      <input type="submit" value="Submit">
    </form>
  </body>
</html>
